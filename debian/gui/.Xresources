Xft.dpi: 192
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! ===== xterm ========
*VT100.utf8Fonts.font2:	-misc-fixed-medium-r-normal--20-200-75-75-c-100-iso10646-1
*VT100.utf8Fonts.font3:	-misc-fixed-medium-r-normal--24-240-75-75-c-140-iso10646-1
*VT100.utf8Fonts.font4:	-misc-fixed-medium-r-normal--34-200-75-75-c-140-iso8859-1
*VT100.utf8Fonts.font5:	-adobe-courier-medium-r-normal--25-180-100-100-m-150-iso8859-1
*VT100.utf8Fonts.font6:	-misc-fixed-medium-r-normal--36-360-75-75-c-260-iso10646-1
*VT100.utf8Fonts.font7: -misc-fixed-medium-r-normal--40-400-75-75-c-300-iso10646-1

xterm*selectToClipboard: true
xterm*dynamicColors: true
xterm.*termName: xterm-256color
*VT100*translations: #override \n\
    Ctrl <Key>minus: smaller-vt-font() \n\
    Ctrl <Key>plus:  larger-vt-font() \n\

    Ctrl Shift <Key>C:  copy-selection(SELECT) \n\
    Ctrl Shift <Key>V:  insert-selection(SELECT)

